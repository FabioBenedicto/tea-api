datasource db {
  provider = "sqlite"
  url      = "file:../tea.db"
}

generator client {
  provider = "prisma-client-js"
}

model user {
    id Int @id @default(autoincrement())
	first_name String
	last_name String
    email String @unique
	password String
    profile_picture_url String?
    notes String?
	emergency_phone_number String?
}
